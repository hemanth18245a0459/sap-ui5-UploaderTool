<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog id="currencyValueHelpDialog"
		noDataText="No Data Found"
		title="Select Currency"
		liveChange=".onCurrencyhandleSearch"
		searchPlaceholder="Select Currency"
		confirm=".onCurrencyValueHelpCancel"
		cancel=".onCurrencyValueHelpCancel"
		showClearButton="true"
		items="{oTableModel>/currencies}">
		<StandardListItem title="{oTableModel>code}"/>
	</SelectDialog>
</core:FragmentDefinition>
